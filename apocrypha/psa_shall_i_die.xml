<!DOCTYPE poem PUBLIC "" "">
<poem variant="ps-apocrypha" unique="shall-i-die" trans="no">
  <titleStmt>
    <title type="common">Shall I Die?</title>
    <title type="main">Shall I Die?</title>
    <title type="short">Shall I Die?</title>
    <title type="abbr">Die</title>
  </titleStmt>
  <authors status="apocrypha">
    <author>William Shakespeare</author>
    <author>John Fletcher</author>
  </authors>
  <fileDesc>
    <publicationStmt>
      <publisher ref="http://www.playshakespeare.com">PlayShakespeare.com</publisher>
      <name type="person" role="editor">Ron Severdia</name>
      <email>william at playshakespeare.com</email>
      <date from="2005" to="2025">2005-2025</date>
